#include <stdio.h>

int isPrime(int b) {
    int flag = 1;
    for (int i=2;i*i<=b;i++) {
        if (b%i==0)
            flag = 0;
    }
    if (flag == 1)
        return 1;
    else
        return 0;
}
int main(void) {
    int n;
    scanf("%d",&n);
    for (int i = 2;i<=n;i++) {
        if (isPrime(i) == 1) {
            printf("%d este numar prim",i);
            printf("\n");
        }

    }
    return 0;
}
